<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <properties resource="mybatis.properties"/>
    <context id="default" targetRuntime="MyBatis3">
        <!-- 序列化pojo -->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <commentGenerator>
            <property name="suppressDate" value="true" />
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <!--<jdbcConnection driverClass="com.mysql.jdbc.Driver"
            connectionURL="jdbc:mysql://localhost:3306/ssm?characterEncoding=utf-8"
            userId="root" password="root" />-->


        <jdbcConnection driverClass="${jdbc.driver}" connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}">
        </jdbcConnection>
        <!--指定生成的实体类-->
        <!-- Model模型生成器,用来生成含有主键key的类，记录类 以及查询Example类
        targetPackage     指定生成的model生成所在的包名
            targetProject     指定在该项目下所在的路径-->
        <javaModelGenerator targetPackage="model"
                            targetProject="src/main/java" />

        <sqlMapGenerator targetPackage="dao"
                         targetProject="src/main/java" />
        <javaClientGenerator targetPackage="dao"
                             targetProject="src/main/java" type="XMLMAPPER" />
        <table schema="manhua" tableName="manhuainfo"    domainObjectName="ManhuaInfo" >
            <!-- 实体类中的成员变量和表的列明一一对应 -->
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table schema="manhua" tableName="manhuacount"    domainObjectName="ManhuaCount" >
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table schema="manhua" tableName="manhuaimg"    domainObjectName="ManhuaImg" >
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table schema="manhua" tableName="message"    domainObjectName="Message" >
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table schema="manhua" tableName="smessage"    domainObjectName="SMessage" >
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table schema="manhua" tableName="user"    domainObjectName="User" >
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table schema="manhua" tableName="history"    domainObjectName="History" >
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table schema="manhua" tableName="collection"    domainObjectName="MCollection" >
            <property name="useActualColumnNames" value="true"/>
        </table>
    </context>



    <!--idea 添加 maven    mybatis-generator:generate -e-->
</generatorConfiguration>